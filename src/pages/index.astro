---
const allpost = await Astro.glob("./posts/*.md*");
import Page from "@/layouts/Page.astro";
---

<Page>
    <ul>
        {
            allpost.map((post) => {
                return (
                    <li>
                        <a href={post.url}>{post.frontmatter.title}</a>
                    </li>
                );
            })
        }
    </ul>
</Page>
